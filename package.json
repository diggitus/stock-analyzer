{
    "name": "stock-analyzer",
    "version": "0.0.0",
    "description": "A tool to analyze stocks",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "build-dev": "ng build --prod --sourcemaps",
        "test": "ng test --watch=false --single-run=true --code-coverage=true",
